    },
    "web": {
      "favicon": "./assets/favicon.png",
      "output": "public",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "823f969c-9ff3-4605-af75-7ad88e82c663"
      }
    }
